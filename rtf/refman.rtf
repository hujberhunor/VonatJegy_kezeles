00031 Vonat beolvas(std::streampos& currPos)\{\par
00032     {\cf17 const} {\cf18 char}* vonatok = {\cf22 "./input/vonatok.txt"};    {\cf20 // const mert megadtam, hogy hol van \'E9s ehhez nem ny\'FAlsz hozz\'E1}\par
00033     std::ifstream file (vonatok);                   {\cf20 // Megynitom a file-t}\par
00034     Vonat von;\par
00035 \par
00036     {\cf19 if} (file.is_open() )\{ \par
00037       {\cf20 // A currpos poziciora ugrom}\par
00038       file.seekg(currPos);\par
00039       {\cf20 // F\'E1jl beolvas\'E1sa egy seg\'E9d strukt\'FAr\'E1ba }\par
00040       Seged seged;\par
00041       file >> seged.szam >> seged.indulo >> seged.veg >> seged.kocsidb >> seged.indulas >> seged.erkezes;\par
00042      \par
00043       {\cf20 // Setterek, mert a konstruktor nem akarta megenni}\par
00044       von.setSzam(atoi(seged.szam));\par
00045       von.setIndulo(seged.indulo);\par
00046       von.setVeg(seged.veg);\par
00047       von.setKocsidb(atoi(seged.kocsidb));\par
00048       von.setIndulas(seged.indulas);\par
00049       von.setErkezes(seged.erkezes);\par
00050 \par
00051       {\cf20 // Minden \'FAj sor kezdet\'E9n\'E9l meg kell \'E1lnom \'E9s el kell t\'E1rolnom}\par
00052       {\cf20 // az \'E9ppenleges kurzor helyzetet, hogy a k\'F6vetkez\'F5 objektumba  }\par
00053       {\cf20 // olvas\'E1s tudja hol kell folytatni. }\par
00054       {\cf18 char} nextChar = file.peek(); \par
00055       {\cf19 if} (nextChar == {\cf23 '\\n'}) \{ \par
00056        currPos = file.tellg(); {\cf20 // Kurzor helyzete. }\par
00057       \} {\cf20 // end of peek}\par
00058     \} {\cf20 // end of if}\par
00059 \par
00060   file.close();\par
00061   {\cf19 return} von; \par
00062 \} {\cf20 // end of beolvas}\par
00063 \par
00064 \par
00065 {\cf20 // Vonatok hozz\'E1ad\'E1sa}\par
